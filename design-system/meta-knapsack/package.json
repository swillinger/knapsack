{
  "name": "@knapsack/meta-knapsack",
  "license": "ISC",
  "version": "2.0.0-beta.42",
  "description": "Knapsack Design System for Knapsack",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production run-s --print-name build:knapsack",
    "build:knapsack": "NODE_ENV=production node $NODE_DEBUG_OPTION ./node_modules/.bin/knapsack build",
    "dev:knapsack": "nodemon --config ../../examples/nodemon.json --watch ../dist/knapsack-design-tokens.json --exec 'yarn start:knapsack'",
    "serve": "NODE_ENV=production node $NODE_DEBUG_OPTION ./node_modules/.bin/knapsack serve",
    "start": "run-s dev:knapsack",
    "start:knapsack": "node $NODE_DEBUG_OPTION ./node_modules/.bin/knapsack start",
    "test": "knapsack test"
  },
  "dependencies": {
    "@knapsack/app": "^2.0.0-beta.42",
    "@knapsack/design-system": "^2.0.0-beta.42",
    "@knapsack/renderer-react": "^2.0.0-beta.42"
  },
  "author": ""
}
